		}
		
		buy_product(id, _qty);
	}
}

function cover(ele, s){
	w = jQuery(ele).outerWidth();
	h = jQuery(ele).height();
	if(s == '' || s == 'add' || s == undefined){
		jQuery(ele).css({'position':'relative'});
		jQuery(ele).append("<div class='cover' style='height:"+h+"px; width:"+w+"px;'><div class='cover-image'></div></div>");
	}
	if(s == 'remove'){
		_cover = jQuery(ele).find('.cover');
		jQuery(_cover).remove();
	}	
}

