				   }else{
				_qty = parseInt(parseInt(qty)+1);
							jQuery("#product_qty_block_"+id+" .product-tile__quick-add-quantity p").remove();
							jQuery("#product_qty_block_"+id+" .product-tile__quick-add-quantity").append("<p class='product-tile__quantity-spinner last'>"+_qty+"</p>");
							
			}
			jQuery(qty_box).val((_qty));
			
		}
		if(i == 'd'){
					jQuery("#product_qty_block_"+id+" .product-tile__quick-add-quantity").removeClass('animate-scroll-up');
					jQuery("#product_qty_block_"+id+" .product-tile__quick-add-quantity").addClass('animate-scroll-down');
					if(qty > 1){
				_qty = parseInt(qty-1);
				jQuery(qty_box).val((_qty));
							
							jQuery("#product_qty_block_"+id+" .product-tile__quick-add-quantity p").remove();
							jQuery("#product_qty_block_"+id+" .product-tile__quick-add-quantity").append("<p class='product-tile__quantity-spinner last'>"+_qty+"</p>");
