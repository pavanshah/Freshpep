function removecartitem(item_id, is_cart){
	/*
         * @Cygnet Infotech
         * @Remove Popup box - Remove Product from Cart
         * @Client Requirement - 30-March 2015
         * if(!confirm("Do you really want to remove this item -1?")){
		return false;
	}*/
	//if(!cart_running){
		cart_running = true;
		
		if(is_cart == true){
			jQuery('#'+item_id+'_cart_item').css({'opacity':'0.2'});
			jQuery('#'+item_id+'_item_remove_button').hide();
		}
		jQuery('.top-cart-container').addClass('active');
		jQuery('.top-cart-box').show();
		cover(jQuery('.cart-items'));
		
		jQuery.ajax({
			url: 'https://www.freshpep.com/quickview/cart/delete',
			data: 'id='+item_id,
			dataType: "json",
			success: function(s){
				//cart_running = false;
				if(s.status == 'SUCCESS'){
					if(s.cart_qty > 0){
						__qty_txt = (s.cart_qty == 1) ? 'Item' : 'Items';
						jQuery('.top-cart-link .ico').html('<b>'+s.cart_qty+'</b>');
					}else{
						jQuery('.top-cart-link .ico').html('');
					}
					jQuery('#cart_container_main').html(s.cart).removeAttr('style');
					
					if(s.cart_qty == 1 && s.pageload == 'refresh'){
						 location.reload();
					}
					
				}
				cover(jQuery('.cart-items'), 'remove');
			},
			error: function(){
				jQuery('#'+item_id+'_item_remove_button').show();
				cart_running = false;
				jQuery('#cart_loader').remove();
				jQuery('#cart_hidden_container').remove();
				cover(jQuery('.cart-items'), 'remove');
			}
		});
	//}
	return false;
}

